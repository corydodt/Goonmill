:aberration
    a c:Family;
    :features [ 
       :hitDice die:d8;
       :babTable pcclass:cleric!:babTable;
       :goodSaves c:will;
       :skillPoints "(int+2)*hitDice";
    ];
    :traits [
        :senses [ c:darkvision [ :range "60ft." ]];
        :proficiencies c:naturalWeapons, c:armorAsDescribed;
    ];
.

:animal
    a c:Family;
    :features [
        :hitDice die:d8;
        :babTable pcclass:cleric!:babTable;
        :goodSaves c:fort, c:ref;
        :skillPoints "(int+2)*hitDice";
    ];
    :traits [
        :ability [ c:int [ :max 2 ]];
        :senses [ c:lowLightVision [] ];
        :alignment c:neutral;
        :treasure [];
        :proficiencies c:naturalWeapons, c:armorNone;
        :additional "noncombative animals -5 to attack, 1/2 str bonus to dam";
    ];
.

:construct
    a c:Family;
    :features [
        :hitDice die:d10;
        :babTable pcclass:cleric!:babTable;
        :goodSaves [];
        :skillPoints "(int+2)*hitDice";
    ];
    :traits [
        :ability [ c:con [ :equal 0 ]];
        :senses [
            c:lowLightVision [];
            c:darkvision [ :range "60ft." ];
        ];
        :immunities c:mindAffecting, c:poison, c:sleep, c:paralysis, c:stunning, 
                    c:disease, c:deathEffects, c:necromancy;
        c:noAbilityDamage [];
        c:noAbilityDrain [];
        c:noEnergyDrain [];
        c:noNonLethalDamage [];
        c:noCriticalHits [];
        c:noTiring [];
        c:noDeathFromMassiveDamage [];
        c:destroyedAtZero [];
        c:soulless [];
        :proficiencies c:naturalWeapons, c:armorNone, c:weaponsAsDescribed;
        :vitalities c:noEat, c:noSleep, c:noBreathe;
        c:fortitudeImmunity [];
        :additional 
            "Cannot heal damage on their own, but often can be repaired by exposing them to a certain kind of effect (see the creature’s description for details) or through the use of the Craft Construct feat. A construct with the fast healing special quality still benefits from that quality.", 
            "Because its body is a mass of unliving matter, a construct is hard to destroy. It gains bonus hit points based on size, as shown on the table. [table]"
        ;
    ];
.

